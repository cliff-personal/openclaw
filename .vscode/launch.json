{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "OpenClaw: Gateway (debug, 18789)",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder:openclaw}",
      "program": "${workspaceFolder:openclaw}/openclaw.mjs",
      "args": ["gateway", "--port", "18789", "--force", "--verbose"],
      "runtimeArgs": ["--enable-source-maps"],
      "preLaunchTask": "OpenClaw: build (tsdown)",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "resolveSourceMapLocations": ["${workspaceFolder:openclaw}/**", "!**/node_modules/**"],
      "outFiles": ["${workspaceFolder:openclaw}/dist/**/*.js"]
    }
  ]
}
